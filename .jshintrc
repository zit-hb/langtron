{
    "esversion": 6,
    "camelcase": true,
    "curly"    : true,
    "eqeqeq"   : true,
    "es3"      : false,
    "forin"    : true,
    "freeze"   : true,
    "immed"    : true,
    "indent"   : 4,
    "latedef"  : true,
    "newcap"   : true,
    "noarg"    : true,
    "noempty"  : true,
    "nonbsp"   : true,
    "quotmark" : "single",
    "undef"    : false,
    "unused"   : false,
    "trailing" : true,
    "predef"   : [
        "window",
        "document",
        "setInterval",
        "clearInterval",
        "setTimeout",
        "clearTimeout",
        "confirm",
        "console",
        "navigator",
        "JSON",
        "angular",
        "require",
        "module",
        "process"
    ]
}
